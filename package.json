        "@radix-ui/themes": "3.2.1",
        "@solana-program/system": "^0.8.1",
        "@solana/kit": "3.0.3",
        "@solana/react": "3.0.3",
        "@solana/web3.js": "^1.98.4",
        "@wallet-standard/core": "^1.1.1",
